      label="Last trade price"
    />
    <HeaderItem
      amount={10.05}
      className="cr-market-info__values-item"
      label="24 hour price"
      sign="+"
    />
    <HeaderItem
      amount={2.345}
      className="cr-market-info__values-item"
      currency="btc"
      label="24 hour value"
    />
  </div>
